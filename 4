#include <iostream>;
using namespace std;

int main() {
	for (int i = 0; i < 13; i++) {
		if (0 <= i && i < 6) {
			for (int j = 0; j < 8; j++) {
				cout << " *";
			}
			cout << " ";
			for (int j = 0; j < 34; j++) {
				cout << "=";
			}
		}
		else {
			for (int j = 0; j < 51; j++) {
				cout << "=";
			}
		}
		cout << "\n";
	}
}
